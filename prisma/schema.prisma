generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Users {
  id              String   @id @map("_id") @db.ObjectId
  first_name      String
  last_name       String?
  email           String   @unique
  profile_picture String?
  createdAt       DateTime @default(now())
}
